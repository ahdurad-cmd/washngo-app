{
  "version": 2,
  "name": "washngo-app",
  "builds": [
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/app", "headers": { "Cache-Control": "no-store, must-revalidate" } },
    { "src": "/docs/(.*)", "dest": "/docs" }
  ],
  "env": {
    "NEXT_PUBLIC_PLAUSIBLE_DOMAIN": "",
    "NEXT_PUBLIC_GA_ID": ""
  }
}
